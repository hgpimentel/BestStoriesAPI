{
	"HackerNewsApiEndpoint": "https://hacker-news.firebaseio.com",
	"HackerNewsApiVersion": "v0",
	"TopStoriesSize": 20,
	"TopStoriesCacheTimeInMinutes": 1,
	"MemoryCacheOptions": {
		"CompactionPercentage": 0,
		"ExpirationScanFrequency": "0:01:00",
		"SizeLimit": "1000"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Warning"
		}
	},
	"AllowedHosts": "*"
}
